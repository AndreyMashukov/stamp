{
    "name": "andrey-mashukov/stamp",
    "keywords": ["photo stamp", "text stamp", "text on photo"],
    "homepage": "https://github.com/AndreyMashukov/stamp",
    "description": "Stamp",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Mashukov",
            "email": "a.mashukov@yandex.ru",
            "role": "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "AdService\\": ["src"]
        },
        "classmap": [
            "src"
        ]
    },
    "require": {
        "php": ">=7.0",
        "ext-gd": "*",
        "ext-imagick": "*"

    },
    "autoload-dev" : {
        "psr-4" : {
            "AdService\\": ["tests"]
        }
    },
    "require-dev": {
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "phploc/phploc": "^2.1",
        "sebastian/phpcpd": "^2.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mamuz/php-dependency-analysis": "^0.6",
        "phpunit/phpunit": "^4.8",
        "logics/codesniffer": "^0.1||dev-trunk",
        "logics/extcheck": "^0.1||dev-trunk"
    },
    "prefer-stable": true
}